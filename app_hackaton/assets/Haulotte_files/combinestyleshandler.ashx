.contact-form{position:relative;}
.contact-form li{margin-bottom:10px;}
.contact-button{padding-top:5px;text-align:right;}
.contact-cimg{display:block;margin-bottom:5px;border:solid 1px #999999;}
.contact-captcha .contact-input{width:202px;margin-bottom:10px;}
.contact-form .popup-overlay{display:none;position:fixed;z-index:101;left:0;top:0;width:100%;height:100%;background:#000000;opacity:0.5;}
.modal-window .contact-form .popup-overlay{background-color:#FFFFFF;opacity:0.5;}
.contact-form .popup-wrapper{display:none;position:absolute;z-index:102;bottom:20px;left:20px;-moz-border-radius:4px;-webkit-border-radius:4px;border-radius:4px;border:1px solid #666666;width:340px;}
.contact-form .popup-wrapper .wait,.contact-form .popup-wrapper .result{display:none;margin-left:auto;margin-right:auto;font-size:1.2em;background-color:#FFFFFF;padding:10px;-moz-border-radius:4px;-webkit-border-radius:4px;border-radius:4px;}
.contact-form .popup-wrapper .wait{height:52px;line-height:52px;padding:0 10px 0 52px;background-image:url(/images/loading_ani_32.gif);background-position:10px 10px;background-repeat:no-repeat;cursor:progress;}
.contact-form .popup-wrapper .result .message{padding:0 10px;}
.contact-form .popup-wrapper .result .message{text-align:right;}
.contact-form .popup-wrapper .result button{margin:10px 0 5px 0;}
#crCaptchaVerif{}
.contact-form .popup-wrapper .result.error{border:solid 1px #821c1c;background-color:#ffe1e1;}
.contact-form .popup-wrapper .result.success .message{text-align:center;margin-bottom:25px;font-size:16px;}
.contact-form .popup-wrapper .result.error .message{color:#821c1c;text-align:center;}
.contact-form .popup-wrapper .close{cursor:pointer;height:27px;position:relative;}
.contact-form .phone-prefix{width:85px;float:left;border-top-right-radius:0 !important;-moz-border-top-right-radius:0 !important;-webkit-border-top-right-radius:0 !important;border-bottom-right-radius:0 !important;-moz-border-bottom-right-radius:0 !important;-webkit-border-bottom-right-radius:0 !important;border-right:none !important;}
.phone-prefix select{width:20px;float:right;}
.phone-prefix .select-text{text-overflow:initial;line-height:40px;padding-left:10px;font-weight:normal;color:#000000;}
.contact-form .phone-prefix-div{width:213px;width:calc(100% - 85px);width:-webkit-calc(100% - 85px);width:-moz-calc(100% - 85px);display:inline-block;}
.phone-prefix-div input{border-top-left-radius:0 !important;-moz-border-top-left-radius:0 !important;-webkit-border-top-left-radius:0 !important;border-bottom-left-radius:0 !important;-moz-border-bottom-left-radius:0 !important;-webkit-border-bottom-left-radius:0 !important;}
#cf-similar-ads .modal-window{width:690px;margin-left:-330px;overflow:auto;}
#cf-similar-ads .modal-inner-wrap{padding:20px;}
#cf-similar-ads .wait-overlay{display:none;position:absolute;left:0;top:0;width:1px;height:1px;background-color:#ffffff;opacity:0.5;z-index:10;}
#cf-similar-ads .wait-icon{display:none;position:absolute;left:0;top:0;width:100%;height:100%;background-image:url(/images/loading_ani_32.gif);background-position:center center;background-repeat:no-repeat;z-index:11;}
#cf-similar-ads .alert{margin-top:10px;}
#cf-similar-ads .select-header{border-bottom:dashed 1px #afafaf;padding-bottom:6px;margin-bottom:6px;}
#cf-similar-ads .items{display:block;margin:0;padding:0;}
#cf-similar-ads .items li{float:left;list-style-type:none;width:126px;}
#cf-similar-ads .items .pic,#cf-similar-ads .items .text,#cf-similar-ads .items .text span{width:120px;display:block;overflow:hidden;}
#cf-similar-ads .items .pic{position:relative;height:90px;margin-bottom:3px;}
#cf-similar-ads .pic img{max-width:120px;max-height:90px;}
#cf-similar-ads input[type=checkbox]{display:none;}
#cf-similar-ads .items label{width:auto;position:initial;}
#cf-similar-ads .items label:before{content:"";display:inline-block;width:20px;height:20px;position:absolute;left:4px;top:4px;background-color:#f5f5f5;border-radius:3px;border:solid 1px #fd6500;}
#cf-similar-ads input[type=checkbox]:checked + label:before{content:"\2713";text-shadow:1px 1px 1px rgba(0,0,0,.2);font-size:20px;font-weight:bold;color:#fd6500;text-align:center;line-height:15px;}
#cf-similar-ads .bottom-links .contact-submit{margin-bottom:10px;}
@media only screen and (max-width:1059px){.contact-form .popup-wrapper{width:100%;bottom:10px;left:0px;}
#crCaptchaVerif{transform:scale(0.85);-webkit-transform:scale(0.85);transform-origin:0 0;-webkit-transform-origin:0 0;}
}
@media only screen and (max-width:767px){.contact-form .popup-wrapper{width:100%;}
#cf-similar-ads .items li{width:100%;margin-bottom:10px;}
#cf-similar-ads .pic,#cf-similar-ads .text{float:left;padding-right:10px;}
}
.location-browser .level-1,.location-browser .level-2,.location-browser .level-3,.location-browser .level-4,.location-browser .title-wrap{border-width:0;}
.location-browser .header{font-size:14px;line-height:34px;}
.location-browser .has-children .header{cursor:pointer;}
.location-browser .title-wrap{}
.location-browser .checkbox{display:inline-block;padding:0 15px 0 15px;cursor:default;}
.location-browser .checkbox input{margin:0;}
.location-browser input.disabled{opacity:0.5;}
.location-browser .toggle-children{display:inline-block;padding:0 5px 0 10px;}
.location-browser .toggle-children i{font-size:12px;font-weight:normal;}
.location-browser .title{font-weight:bold;}
.location-browser .checkbox-controls{padding:2px;line-height:1;}
.location-browser .open>.header .checkbox-controls{display:block;}
.location-browser .level-2 .header,.location-browser .has-children .level-2 .header{cursor:default;}
.location-browser .level-2 .has-children .header:hover{background-color:#f4feff;cursor:pointer;}
.location-browser .level-2 .title{font-weight:normal;}
.location-browser .level-3{border-top-width:1px;padding:10px;}
.location-browser .level-3 li{border-width:0;}
.location-browser .level-3 .header,.location-browser .has-children .level-3 .header:hover{background-color:#ffffff;cursor:default;}
.location-browser .level-3 .header{line-height:1;cursor:default;}
.location-browser .level-3 .has-children .header:hover{cursor:pointer;}
.location-browser .level-3 .has-children .header:hover .title{text-decoration:underline;}
.location-browser .level-3 .checkbox{border-width:0;padding:2px 5px 2px 5px;}
.location-browser .level-3 .toggle-children{padding:0 5px 0 0;}
