.image-gallery{margin:5px 0px 20px 0px;background:#fff;overflow:hidden;border:1px solid #d9d9d9;-moz-border-radius:3px;border-radius:3px;width:100%;display:block;float:left;}
.image-gallery:empty{display:none;}
.image-gallery ul{margin:0px;padding:0px;}
.image-gallery ul li{float:left;margin:8px;list-style-type:none;position:relative;width:110px;height:110px;line-height:110px;border:1px solid #d9d9d9;text-align:center;-moz-border-radius:3px;border-radius:3px;box-shadow:0px 2px 6px #909090;background-color:white;}
.video-gallery ul li{height:82px;line-height:82px;padding-top:20px;}
.loading,.loading:empty{background:url('/images/loading_ani_32.gif') 50% 50% no-repeat;background-color:#ffffff;min-height:52px;display:block;}
.loading ul{visibility:hidden;}
.image-gallery ul li img{max-height:105px;max-width:110px;vertical-align:middle;}
.image-gallery .controls{background:#f6f6f6;border-bottom:1px solid #d9d9d9;-moz-border-radius-top:3px;border-top-right-radius:3px;border-top-left-radius:3px;height:20px;width:100%;position:absolute;top:0;left:0;text-align:left;line-height:20px;opacity:0.8;filter:alpha(opacity=80);z-index:2;}
.image-gallery ul li .controls span{display:block;height:20px;float:left;border-right:1px solid #d9d9d9;text-decoration:none;color:black;padding:0px 5px 0px 5px;}
.image-gallery ul li .controls a{display:block;height:20px;float:left;border-right:1px solid #d9d9d9;text-decoration:none;color:black;padding:0px 5px 0px 5px;}
.image-gallery ul li .controls a.rotate{background:#f6f6f6;position:absolute;top:88px;padding:0px 2px 0px 2px;border:none;border-bottom-left-radius:3px;-moz-border-radius-top:3px;font-size:x-large;}
.image-gallery ul li .controls a.delete{float:right;border-right:none;border-left:1px solid #d9d9d9;}
.image-gallery ul li .controls a.inactive{opacity:0.1;filter:alpha(opacity=10);}
.add_images_button input#fileupload{display:block;cursor:pointer;direction:ltr;font-size:250px;width:100%;margin:0;opacity:0;position:absolute;left:0;top:0;z-index:9999;}
.add_images_button{overflow:hidden;position:relative;}
.add_images_button.disabled{opacity:0.7;}
.add_images_button.disabled .image_uploader_button{cursor:not-allowed;}
.file-counter{float:right;font-weight:bold;font-size:large;}
