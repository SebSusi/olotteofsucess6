body{background-color:#ffffff;background-repeat:repeat-x;}
body,iframe,form,fieldset,ol,ul{border:none;margin:0;padding:0;}
body,th,td{font-family:'Open Sans',sans-serif;font-size:13px;color:#333333;}
a{cursor:pointer;outline:0;border:0;color:#333333;}
a:focus,:focus{outline:0;}
ol{list-style:decimal !important;list-style-position:inside !important;}
ul{list-style:none;margin:0;padding:0;}
table,caption,tbody,tfoot,thead,tr,th,td{border:0;border-collapse:collapse;font-family:inherit;font-size:inherit;}
table{border-collapse:collapse;}
th{font-weight:bold;text-align:left;}
hr{background-color:#e3e3e3;border:none;color:#e3e3e3;height:1px;}
img{border:none;}
h1:first-child,h2:first-child,h3:first-child,h4:first-child,h5:first-child,h6:first-child,p:first-child{margin-top:0;}
h1:last-child,h2:last-child,h3:last-child,h4:last-child,h5:last-child,h6:last-child{margin-bottom:0;}
button{border:none;}
option{padding:2px;}
table.border,table.border th,table.border td,th.border,td.border{border:1px solid #e3e3e3;}
a.black-link,span.black-link{text-decoration:underline;color:#000000;}
a.white-link,span.white-link{text-decoration:underline;color:#ffffff;}
a.orange-link,span.orange-link{text-decoration:underline;color:#fb7121;}
a.blue-link,.blue-link{color:#2296ba !important;text-decoration:none !important;font-size:16px;cursor:pointer;}
a.blue-link:hover,.blue-link:hover,a.blue-link-small:hover,.blue-link-small:hover{text-decoration:underline !important;}
a.blue-link-small,.blue-link-small{color:#2296ba !important;text-decoration:none !important;}
input[type=text],input[type=password],input[type=url],input[type=tel],input[type=number],input[type=color],input[type=email],input[type=date],textarea,select,.select-box{height:40px;line-height:40px;border:1px solid #d5d5d5;border-radius:3px;position:static;-moz-border-radius:3px;-webkit-border-radius:3px;width:100%;padding:1px;padding-left:5px;font-family:"Open Sans",sans-serif;font-size:14px;color:#000000;background:#fff;box-sizing:border-box;-moz-box-sizing:border-box;-webkit-box-sizing:border-box;}
input[type=text]:focus,input[type=password]:focus,input[type=url]:focus,input[type=tel]:focus,input[type=number]:focus,input[type=color]:focus,input[type=email]:focus,input[type=date]:focus,textarea:focus,.primary-button:focus,.secondary-button:focus,.page-numbers li:focus,select:focus,.select-box:focus{}
input[type=checkbox]:focus,input[type=radio]:focus{box-shadow:0 0 5px #298fc6;-moz-box-shadow:0 0 5px #298fc6;-webkit-box-shadow:0 0 5px #298fc6;}
input[type=button]{border:none;}
input.error,select.error,.select-box.error,textarea.error,ul.error{box-shadow:0 0 4px #ea3232;-moz-box-shadow:0 0 4px #ea3232;-webkit-box-shadow:0 0 4px #ea3232;border:1px solid #f71414;outline:none !important;}
label.error,span.error{color:#f71414;}
.range-select input{float:left;width:45%;}
.range-select .sep{float:left;width:10%;text-align:center;line-height:28px;}
.select-box select{z-index:2;position:relative;border:none;background:none;outline:none;padding:4px 12px 4px 32px;opacity:0;-webkit-appearance:none;filter:alpha(opacity=0);width:100% !important;height:100% !important;padding:0;margin:0;}
.select-box{position:relative;display:inline-block;*display:inline;zoom:1;box-sizing:border-box;-webkit-box-sizing:border-box;-moz-box-sizing:border-box;cursor:default;}
.select-text{z-index:1;position:absolute;left:0;display:inline-block;zoom:1;padding-top:0;background:transparent;height:inherit;line-height:inherit;top:0px;color:#535353;font-weight:bold;white-space:nowrap;overflow:hidden;text-overflow:ellipsis;padding-left:10px;padding-right:18px;box-sizing:border-box;-webkit-box-sizing:border-box;-moz-box-sizing:border-box;width:100%;cursor:default;}
.select-arrow{display:block;position:absolute;right:5px;top:50%;margin-top:-6px;height:12px;font-size:12px;line-height:12px;color:#666666;z-index:1;cursor:default;}
textarea{height:auto;line-height:normal;resize:vertical;}
input[type=submit]{width:auto;border:0;margin-top:15px;cursor:pointer;}
.contact-submit,.search-submit{width:100%;box-shadow:inset 0 1px 0 0 #ffffff;font-size:16px;border:solid 1px #d85c1e;font-weight:bold;}
.contact-submit:hover,.search-submit:hover{border-color:#822b00;}
.contact-submit:active,.search-submit:active{box-shadow:inset 0 1px 2px 1px rgba(0,0,0,0.3);border:solid 1px #d85c1e;}
label,.field-label,.formFieldHeader{display:inline-block;margin-bottom:2px;color:#333333;width:100%;position:relative;}
label.quarter{width:19%;margin-top:0px;height:30px;line-height:30px;clear:left;}
label .parenth{font-weight:normal;}
.sub-label,.sub-label input{width:auto !important;font-weight:normal;}
.sub-label{margin-top:3px;}
.sub-label input{margin-left:0;}
.sub-label{margin-right:10px;}
